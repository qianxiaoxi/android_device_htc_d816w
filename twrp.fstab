/system 	ext4 	/dev/block/platform/msm_sdcc.1/by-name/system 		flags=display="系统分区(system)"
/data 		ext4 	/dev/block/platform/msm_sdcc.1/by-name/userdata 	flags=display="数据分区(data)"	length=-16384
/cache 		ext4 	/dev/block/platform/msm_sdcc.1/by-name/cache		flags=display="缓存分区(cache)"
/boot		emmc 	/dev/block/platform/msm_sdcc.1/by-name/boot		flags=display="内核分区(boot)"
/recovery	emmc 	/dev/block/platform/msm_sdcc.1/by-name/recovery


/external_sd	vfat	/dev/block/mmcblk1p1					flags=display="外置SD卡(external_sd)";storage;wipeingui;removable

/usbdisk	        vfat	/dev/block/sda1						flags=display="OTG-U盘(usbdisk)";storage;wipeingui;removable
